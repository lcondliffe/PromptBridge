{
  "name": "promptbridge",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "predev": "node scripts/db-init.mjs",
    "dev": "next dev",
    "build": "pnpm prisma:generate && next build",
    "prestart": "node scripts/db-init.mjs",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "pnpm prisma:generate",
    "prisma:generate": "prisma generate --schema packages/api/prisma/schema.prisma",
    "prisma:db:push": "prisma db push --schema packages/api/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema packages/api/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema packages/api/prisma/schema.prisma",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=github"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.8",
    "@prisma/client": "^5.22.0",
    "@promptbridge/api": "workspace:*",
    "@promptbridge/sdk": "workspace:*",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "posthog-js": "^1.262.0",
    "posthog-node": "^5.8.4",
    "prismjs": "^1.30.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "refractor": "^4.8.1",
    "rehype-prism-plus": "^2.0.1",
    "remark-gfm": "^4.0.1",
    "simple-icons": "^15.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@clerk/testing": "^1.12.4",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "cross-spawn": "7.0.6"
    }
  }
}
